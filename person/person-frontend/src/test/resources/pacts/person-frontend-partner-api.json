{
  "consumer": {
    "name": "person-frontend"
  },
  "provider": {
    "name": "partner-api"
  },
  "interactions": [
    {
      "description": "create partner",
      "request": {
        "method": "POST",
        "path": "/person/rest/api/v1/partners",
        "body": {
          "name": "Einstein",
          "forename": "Albert"
        }
      },
      "response": {
        "status": 201,
        "headers": {
        },
        "body": {
          "id": "P87654321",
          "name": "Einstein",
          "forename": "Albert"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "regex",
            "regex": "P[0-9]{8}"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}